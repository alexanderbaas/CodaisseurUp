<div class="container">
<div class="row">
  <div class="col-md-3">
    
  </div>
  <div class="col-md-7">
    <div class="panel panel-danger">
      <div class="panel-heading">
        Your Listings
        </div>

      <div class="panel-body">
        <% @events.each do |event| %>
        <div class="row">
          <div class="col-md-7">
            <h4><%= event.name %></h4>
            </div>
            <div class="col-md-3 right">
            <%= link_to "View", event, class: "btn btn-danger" %>
            <%= link_to "Update", edit_event_path(event), class: "btn btn-danger" %>
            </div>
          </div>
        <% end %>
      </div>
    </div>
    <%= link_to "Add new place", new_event_path, class: "btn btn-danger" %>

  </div>
</div>
